    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Attach",
            "type": "python",
            "request": "attach",
            "port": 5678,
            "host": "localhost"
        },
        {
            "name": "Django runserver",
            "type": "python",
            "request": "launch",
            "python": "/venv/bin/python3",
            "program": "/venv/bin/manage.py",
            "console": "integratedTerminal",
            "args": [
                "runserver",
                "--noreload",
                "--nothreading",
                "0.0.0.0:8001"
            ],
            "django": true
        },
        {
            "name": "Django test",
            "type": "python",
            "request": "launch",
            "python": "/venv/bin/python3",
            "program": "/venv/bin/django-admin",
            "console": "integratedTerminal",
            "args": [
                "test"
            ],
            "django": true
        },
    ]
}
